{
  "name": "sconn-client",
  "version": "1.0.4",
  "description": "SConn WebSocket client library with state machine, reconnection and data caching",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hanxi/sconn-client.git"
  },
  "bugs": {
    "url": "https://github.com/hanxi/sconn-client/issues"
  },
  "homepage": "https://github.com/hanxi/sconn-client#readme",
  "scripts": {
    "build": "tsc && bun build src/index.ts --outdir dist --target browser --format esm --sourcemap",
    "prepublishOnly": "bun run build",
    "dev": "bun --watch src/index.ts",
    "type-check": "bun tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server": "bun run test-server.ts",
    "server:dev": "bun --watch test-server.ts",
    "example:conn": "bun run examples/conn-example.ts",
    "example:sconn": "bun run examples/sconn-example.ts",
    "example:network": "bun run examples/network-example.ts"
  },
  "keywords": [
    "websocket",
    "client",
    "browser",
    "sproto",
    "goscon",
    "typescript"
  ],
  "author": "涵曦",
  "license": "MIT",
  "dependencies": {
    "@imhanxi/sproto-js": "^0.1.7",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/ws": "^8.5.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
